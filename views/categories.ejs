<%- include('partials/_header.ejs') %>

  <!-- <main> -->
    <h1 class="title-header">Category List</h1>
    <!-- <div class="main-container"> -->
      <section class="resources-main my-resources">
        <% categories.forEach(category=> { %>
          <a href="/resources/category/<%= category.id %>" class="resource-card-link all <%=category.category_name%>">
            <div class="resource-card">
              <div class="card-content">
                <img src="<%= category.category_img %>" alt="thumbnail image" class="thumbnail_img" />
              </div>


              <div class="resource-card-title-detail">
                <div class="category-title">
                  <%= category.category_name %>
                </div>
              </div>
            </div>
          </a>
          <% }); %>
      </section>
    <!-- </div> -->
  <!-- </main> -->

